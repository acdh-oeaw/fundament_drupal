
<div class="container-fluid">
    {% if primary %}
        <h2 class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
        <ul class="d-flex flex-wrap pl-0">
            {{ primary }}
        </ul>
    {% endif %}
    {% if secondary %}
        <h2 class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
        <ul class="d-flex flex-wrap pl-0">
            {{ secondary }}
        </ul>
    {% endif %}
</div>